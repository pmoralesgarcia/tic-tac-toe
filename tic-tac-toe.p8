pico-8 cartridge // http://www.pico-8.com
version 39
__lua__
function _init()
 poke(0x5f2d, 1)
end

function _update()
 mousex = stat(32)
 mousey = stat(33)
end

function _draw()
 cls()
 line(mouse, mousey - 4, mousex, mouse + 4)
 line(mousex - 4, mousey, mousex + 4, mousey)
end

function draw_grid()
 line(42, 0, 127, 42)
 line(0, 85, 127, 85)
end

function draw_nought(x, y)
 circ(x, y, 10)
end

function draw_cross(x, y)
 line(x - 10, y - 10, x + 10)
 line(x + 10, y - 10, x + 10)
end

p = {"", "", "", "", "", "", "", "", ""}


